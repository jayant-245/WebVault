{
  "manifest_version": 3,
  "name": "WebVault",
  "version": "1.0.0",
  "description": "save and organize your favorite websites with notes, all in one place.",
  "action": {
    "default_title": "Open WebVault"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}